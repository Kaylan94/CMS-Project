{
  "name": "cms_server",
  "version": "1.0.0",
  "description": "server for cms ",
  "main": "index.js",
  "dependencies": {
    "bcrypt": "5.0.1",
    "body-parser": "1.19.0",
    "concurrently": "6.4.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "helmet": "4.6.0",
    "jsonwebtoken": "8.5.1",
    "mongoose": "6.0.13",
    "nodemon": "2.0.14",
    "path": "0.12.7",
    "react": "17.0.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "8.5.5"
  },
  "scripts": {
    "test": "mocha",
    "server": "nodemon server.js",
    "client": "cd palmsoft-cms && npm start",
    "client-install": "cd palmsoft-cms && npm install",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "npm run server",
    "build": "cd palmsoft-cms && npm run build",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install && npm run client-install && npm run build"
  },
